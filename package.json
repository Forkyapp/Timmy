{
  "name": "jarvis-task-automation",
  "version": "1.0.0",
  "description": "JARVIS-style autonomous task automation system",
  "main": "devin.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node devin.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/*.test.js"
    ]
  }
}
