{
  "name": "timmy-cli",
  "version": "2.0.0",
  "description": "Autonomous task automation: ClickUp to GitHub via AI",
  "main": "dist/timmy.js",
  "bin": {
    "timmy": "dist/timmy.js"
  },
  "files": [
    "dist/",
    "templates/data/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "setup": "bash docker/scripts/setup-auth.sh",
    "start": "bash scripts/smart-start.sh",
    "start:local": "npm run build && NODE_NO_WARNINGS=1 node dist/timmy.js",
    "start:docker": "bash scripts/smart-start.sh --docker",
    "docker:build": "bash docker/scripts/build-base.sh && docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "init": "ts-node scripts/interactive-setup.ts",
    "settings": "ts-node scripts/settings.ts",
    "build": "tsc && tsc-alias && cp src/infrastructure/storage/schema.sql dist/src/infrastructure/storage/schema.sql",
    "dev": "ts-node -r tsconfig-paths/register timmy.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "switch": "ts-node scripts/switch-project.ts",
    "projects": "ts-node scripts/list-projects.ts",
    "current": "ts-node scripts/current-project.ts",
    "build:context-index": "ts-node scripts/build-context-index.ts",
    "migrate:json-to-sqlite": "ts-node scripts/migrate-json-to-sqlite.ts",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cli",
    "automation",
    "clickup",
    "github",
    "ai",
    "claude",
    "task-management",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forkyapp/Timmy.git"
  },
  "bugs": {
    "url": "https://github.com/Forkyapp/Timmy/issues"
  },
  "homepage": "https://github.com/Forkyapp/Timmy#readme",
  "author": "Forkyapp",
  "license": "MIT",
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.4.1",
    "commander": "^12.1.0",
    "discord.js": "^14.24.2",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.23",
    "openai": "^6.9.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/types$": "<rootDir>/src/types",
      "^@/types/(.*)$": "<rootDir>/src/types/$1",
      "^@/shared/(.*)$": "<rootDir>/src/shared/$1",
      "^@/core/(.*)$": "<rootDir>/src/core/$1",
      "^@/infrastructure/(.*)$": "<rootDir>/src/infrastructure/$1",
      "^@/config/(.*)$": "<rootDir>/src/config/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/tmp/",
      "/codex-review-clone/",
      "/*-worktree/",
      "/.timmy-worktrees/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/codex-review-clone/",
      "/*-worktree/",
      "/.timmy-worktrees/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!src/test-setup.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 22,
        "branches": 15,
        "functions": 21,
        "lines": 22
      }
    },
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "json-summary"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
